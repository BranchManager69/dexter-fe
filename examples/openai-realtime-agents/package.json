{
  "name": "realtime-examples",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build && NODE_ENV=production NODE_OPTIONS=--enable-source-maps pm2 restart dexter-agents --update-env -- run start -- --hostname 0.0.0.0 --port 3210",
    "pm2:prod": "NODE_ENV=production NODE_OPTIONS=--enable-source-maps pm2 restart dexter-agents --update-env -- run start -- --hostname 0.0.0.0 --port 3210",
    "pm2:dev": "pm2 restart dexter-agents --update-env -- run dev -- --hostname 0.0.0.0 --port 3210",
    "dexchat": "node scripts/dexchat.js"
  },
  "bin": {
    "dexchat": "./scripts/dexchat.js"
  },
  "dependencies": {
    "@openai/agents": "^0.1.3",
    "@openai/agents-realtime": "^0.1.3",
    "@radix-ui/react-icons": "^1.3.2",
    "dotenv": "^16.4.7",
    "next": "^15.3.1",
    "openai": "^4.77.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3",
    "uuid": "^11.0.4",
    "yargs": "^18.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "playwright": "^1.55.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}

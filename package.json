{
  "name": "dexter-fe",
  "private": true,
  "version": "0.1.0",
  "description": "Dexter Frontend (Next.js + Realtime)",
  "scripts": {
    "dev": "next dev -p 3017",
    "build": "next build",
    "start": "next start -p 3017",
    "harness": "node ./cli/realtime-harness/run.mjs",
    "dexter-stream:start": "pm2 start scripts/dexter-stream/run.mjs --name dexter-stream",
    "dexter-stream:stop": "pm2 stop dexter-stream",
    "dexter-stream:restart": "pm2 restart dexter-stream",
    "dexter-stream:status": "pm2 show dexter-stream",
    "docs:serve": "honkit serve docs --port 3055",
    "docs:build": "honkit build docs docs/_book",
    "docs:deploy": "bash docs/tools/docs-deploy.sh"
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^1.3.1",
    "@openai/agents": "^0.1.0",
    "@openai/agents-realtime": "^0.1.0",
    "@supabase/supabase-js": "^2.45.1",
    "livekit-server-sdk": "^1.2.7",
    "next": "^15.0.0-canary",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "type": "module",
  "devDependencies": {
    "@types/react": "19.1.12",
    "playwright": "^1.55.1",
    "typescript": "^5.6.2",
    "honkit": "^6.1.4"
  }
}

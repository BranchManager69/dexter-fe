{
  "name": "dexter-fe",
  "private": true,
  "version": "0.1.0",
  "description": "Dexter Frontend (Next.js + Realtime)",
  "scripts": {
    "dev": "next dev -p 3017",
    "build": "next build",
    "start": "next start -p 3017",
    "harness": "node ./cli/realtime-harness/run.mjs",
    "agents:dev": "npm --prefix examples/openai-realtime-agents run pm2:dev",
    "agents:prod": "npm --prefix examples/openai-realtime-agents run pm2:prod",
    "agents:deploy": "npm --prefix examples/openai-realtime-agents run deploy"
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^1.3.1",
    "@openai/agents": "^0.1.0",
    "@openai/agents-realtime": "^0.1.0",
    "@supabase/supabase-js": "^2.45.1",
    "next": "^15.0.0-canary",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "type": "module",
  "devDependencies": {
    "@types/react": "19.1.12",
    "typescript": "^5.6.2"
  }
}
